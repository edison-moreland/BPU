library(logicworld) {
    cell(BUF) {
        area: 6;
        pin(A) { direction: input; }
        pin(Y) { direction: output;
                 function: "A"; }
    }
    cell(NOT) {
        area: 3;
        pin(A) { direction: input; }
        pin(Y) { direction: output;
                 function: "A'"; }
    }
    cell(NAND) {
        area: 4;
        pin(A) { direction: input; }
        pin(B) { direction: input; }
        pin(Y) { direction: output;
                 function: "(A*B)'"; }
    }
    cell(NOR) {
        area: 4;
        pin(A) { direction: input; }
        pin(B) { direction: input; }
        pin(Y) { direction: output;
                 function: "(A+B)'"; }
    }

    cell(DLATCH) {
        area : 5;
        latch ("IQ","IQN") {
            enable: "G";
            data_in: "D";
        }

        pin(D) { direction: input; }
        pin(G) { direction: input; }
        pin(Q) { direction: output;
                 function: "IQ";
                 internal_node: "Q"; }
    }
}